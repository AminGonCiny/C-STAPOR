#include <vector>
#include <stdlib.h>
#include <iostream>
#include <random>
using namespace std;

void shellsort(int* tab, int left, int right)
{
	int inner, outer, temp, h = 1, numElements = right + 1;
	while (h <= numElements / 3)
		h = h * 3 + 1;
	while (h > 0);
	{
		for (outer = h; outer < numElements; outer++)
		{
			temp = tab[outer];
			inner = outer;
			while (inner > h - 1 && tab[inner - h] >= temp)
			{
				tab[inner] = tab[inner - h];
				inner = inner - h;
				
			}
			tab[inner] = temp;
		}
		h = (h - 1) / 3;
	}
}


void bubbleSort(vector<int>& v) {
	int n = v.size();
	for (int i = 0; i < n - 1; i++) {

		for (int j = 0; j < n - i - 1; j++) {
			if (v[j] > v[j + 1])
				swap(v[j], v[j + 1]);
		}
	}
}

void insertionSort(vector<int>& v) {
	int n = v.size();
	for (int i = 1; i < n; i++) {
		int key = v[i];
		int j = i - 1;
		while (j >= 0 && v[j] > key) {
			v[j + 1] = v[j];
			j = j - 1;
		}
		v[j + 1] = key;
	}
}

int main() {
	vector<int>v;
	for (int i = 0; i < 10000; i++) {
		v.push_back(rand() % 100000); 
		}



	//bubbleSort(v);
	insertionSort(v);
	for (auto i : v) {
		cout << i << " ";
	}
	return 0;
}
